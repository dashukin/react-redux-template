{
  "name": "react-redux-template",
  "version": "0.4.12",
  "description": "React redux feature-based template with scalable structure. Includes Webpack, Babel, SCSS support and Dev/Prod build tasks, ESLint",
  "main": "index.js",
  "scripts": {
    "build:app": "webpack --config config/webpack/webpack.config.client.babel.js",
    "build:server": "webpack --config config/webpack/webpack.config.server.babel.js",
    "build:all": "webpack --config config/webpack/webpack.config.all.babel.js",
    "build": "cross-env NODE_ENV=production npm run build:all",
    "build:locale": "node scripts/localization",
    "build:locale:dev": "WATCH=true node scripts/localization",
    "build:inlinescripts": "node ./scripts/inline-scripts/index.js",
    "start": "npm run build:all",
    "server:dev": "nodemon dist/server",
    "server:debug": "nodemon -- --inspect dist/server",
    "test": "cross-env NODE_ENV=test jest --config config/jest/jest.config.js",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run test:coverage && codecov",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config config/jest/jest.config.js",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix",
    "add:store": "plop"
  },
  "repository": "git+https://github.com/dashukin/react-redux-template.git",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "scss",
    "webpack"
  ],
  "author": "Vasili Molakhau <dashukin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashukin/react-redux-template/issues"
  },
  "homepage": "https://github.com/dashukin/react-redux-template#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "codecov": "^3.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-recursive": "0.0.6",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.1.0",
    "jest-enzyme": "^7.1.0",
    "plop": "^2.5.3",
    "redux-saga-test-plan": "^4.0.0-beta.3"
  },
  "dependencies": {
